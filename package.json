    "babel-loader": "^5.3.2",
    "boom": "^2.8.0",
    "classnames": "^2.1.3",
    "css-loader": "^0.16.0",
    "express": "^4.13.3",
    "fastclick": "^1.0.6",
    "file-loader": "^0.8.4",
    "install": "^0.1.8",
    "json-loader": "^0.5.2",
    "postcss-calc": "^4.1.0",
    "postcss-color-function": "^1.3.2",
